    <icon src="resources/ios/icon/icon-72@2x.png" width="144" height="144"/>
    <icon src="resources/ios/icon/icon-76.png" width="76" height="76"/>
    <icon src="resources/ios/icon/icon-76@2x.png" width="152" height="152"/>
    <icon src="resources/ios/icon/icon-83.5@2x.png" width="167" height="167"/>
    <icon src="resources/ios/icon/icon-small.png" width="29" height="29"/>
    <icon src="resources/ios/icon/icon-small@2x.png" width="58" height="58"/>
    <icon src="resources/ios/icon/icon-small@3x.png" width="87" height="87"/>
  </platform>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="android-minSdkVersion" value="21"/>
  <preference name="BackupWebStorage" value="none"/>
  <preference name="SplashScreen" value="screen"/>
  <preference name="SplashScreenDelay" value="30000"/>
  <preference name="AutoHideSplashScreen" value="false"/>
  <preference name="SplashShowOnlyFirstTime" value="false"/>
  <preference name="ShowSplashScreenSpinner" value="false"/>
  <preference name="FadeSplashScreen" value="false"/>
  <preference name="AllowInlineMediaPlayback" value="true"/>
  <feature name="SplashScreen">
    <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen"/>
  </feature>
  <feature name="StatusBar">
    <param name="ios-package" onload="true" value="CDVStatusBar"/>
  </feature>
  <icon src="resources/android/icon/drawable-xhdpi-icon.png"/>
  <plugin name="cordova-plugin-device" spec="~1.1.1"/>
  <plugin name="cordova-plugin-console" spec="~1.0.2"/>
  <plugin name="cordova-plugin-whitelist" spec="~1.2.1"/>
  <plugin name="cordova-plugin-splashscreen" spec="~3.1.0"/>
  <plugin name="cordova-plugin-statusbar" spec="~2.1.0"/>
  <plugin name="ionic-plugin-keyboard" spec="~2.1.0"/>
  <plugin name="cordova-plugin-datepicker" spec="~0.9.3"/>
  <plugin name="cordova-plugin-network-information" spec="~1.2.1"/>
  <plugin name="cordova-plugin-geolocation" spec="~2.2.0"/>
  <plugin name="cordova-plugin-tts" spec="~0.2.3"/>
  <plugin name="org.apache.cordova.speech.speechrecognition" spec="https://github.com/macdonst/SpeechRecognitionPlugin"/>
  <plugin name="cordova-plugin-camera" spec="~2.2.0"/>
  <plugin name="com.cordova.plugins.sms" spec="~0.1.10"/>
  <plugin name="phonegap-plugin-push" spec="~1.7.0">
    <variable name="SENDER_ID" value="693415120998"/>
  </plugin>
  <plugin name="cordova-plugin-app-version" spec="~0.1.8"/>
  <plugin name="cordova-plugin-inappbrowser" spec="~1.4.0"/>
  <plugin name="com.lampa.startapp" spec="~0.1.4"/>
  <plugin name="cordova-plugin-contacts" spec="~2.1.0"/>
  <plugin name="cordova-plugin-pin-dialog" spec="https://github.com/Paldom/PinDialog.git"/>
  <plugin name="de.appplant.cordova.plugin.local-notification" spec="~0.8.4"/>
  <plugin name="com.instabug.cordova.plugin" spec="https://github.com/Instabug/instabug-cordova"/>
  <plugin name="cordova-plugin-customurlscheme" spec="~4.2.0">
    <variable name="URL_SCHEME" value="employeur"/>
  </plugin>
</widget>